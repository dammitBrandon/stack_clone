<h1> WACK OVERFLOW</h1>

<ul>
  <% @questions.each do |question|  %>
  <li>
    <%=link_to question.title, question_path(question) %>
    <br>
    By <%= question.user.name  %>
    <% if question.user_id == session[:id] %>
      <%= button_to 'Edit', edit_question_path(question) %>
    <% end %>
  </li>
  <% end %>
</ul>
